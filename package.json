{
  "name": "nuxt-stencil",
  "version": "0.1.1",
  "description": "Easy Stencil component library integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js",
    "stencil",
    "stencil-js",
    "nuxt-stencil"
  ],
  "main": "lib/module.js",
  "files": [
    "lib"
  ],
  "repository": "https://github.com/Gomah/nuxt-stencil",
  "homepage": "https://github.com/Gomah/nuxt-stencil",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "prepublishOnly": "yarn build",
    "test:system": "NODE_ENV=test jest --testRegex \"/test/system/(.+)\\.test\\.ts$\"",
    "build": "rimraf lib && tsc -b",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "watch": "tsc --watch",
    "test": "yarn test:lint && NODE_ENV=test jest",
    "test:lint": "eslint --ext .js,.ts, src/"
  },
  "dependencies": {
    "consola": "^2.15.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@nuxt/types": "^2.15.5",
    "@types/consola": "^2.2.5",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.6.3",
    "bulmil": "^0.7.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^25.4.0",
    "nuxt": "^2.12.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "ts-jest": "^25.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.9"
  }
}
